{
  "name": "mediaserver",
  "version": "1.0.0",
  "description": "mediasoup server for inclubit 2",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/routers/appRouter.ts"
    }
  },
  "author": "Gunnar Oledal",
  "license": "ISC",
  "scripts": {
    "start": "dotenv -e ../../.env node ./dist/index.js",
    "dev": "dotenv -v DEVELOPMENT=true -e ../../.env -- tsx watch ./src/index.ts",
    "build": "tsup-node --clean",
    "build:watch": "tsup-node --watch",
    "tsc:v": "tsc -v",
    "clean": "tsc --build --clean",
    "test": "jest --watch",
    "test:once": "jest"
  },
  "dependencies": {
    "@vue/reactivity": "^3.2.47",
    "database": "workspace:*",
    "date-fns": "^2.29.3",
    "debug-level": "3.0.0-1",
    "h264-profile-level-id": "^2.0.0",
    "ip": "^1.1.8",
    "lodash-es": "^4.17.21",
    "schemas": "workspace:*",
    "shared-modules": "workspace:*",
    "superjson": "^1.12.2",
    "tiny-typed-emitter": "^2.1.0",
    "tozod": "^3.0.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.43.0"
  },
  "devDependencies": {
    "@types/ip": "^1.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.0.1"
  }
}
